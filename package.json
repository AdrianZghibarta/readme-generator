    "month": "^2.0.1",
    "month-day": "^0.1.1",
    "parse-author": "^1.0.0",
    "pretty-remarkable": "^0.4.0",
    "remarkable": "^1.7.1",
    "template-helpers": "^0.6.7",
    "write": "^0.3.2",
    "yargs-parser": "^4.2.0",
    "year": "^0.2.1"
  },
  "keywords": [
    "generate",
    "generator",
    "readme"
